@inject Microsoft.Extensions.Configuration.IConfiguration _config
@using Microsoft.Extensions.Configuration;
<div class="content-wrapper">
    <section class="content-header">
        <h1>
            首页
            <small>控制面板</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> 首页</a></li>
            <li class="active"></li>
        </ol>
    </section>

    <section class="content">
        <div class="row">
            <div class="col-md-12">
                <div class="box">
                    <div class="box-header with-border">
                        <h3 class="box-title">我管理的仓库</h3>
                        <div class="box-tools pull-right">
                            <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip"
                                    title="Collapse">
                                <i class="fa fa-minus"></i>
                            </button>
                            <button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove">
                                <i class="fa fa-times"></i>
                            </button>
                        </div>
                    </div>
                    <div class="box-body">
                        @{
                            foreach (Wms_warehouse warehouse in (Wms_warehouse[])ViewData["stores"])
                            {
                                <a href="/Home?type=wms&storeId=@warehouse.WarehouseId" style="display:inline-block">
                                    <div style="display:inline-block;width:200px;height:300px;text-align:center;border:1px solid #3c8dbc;">
                                        <div style="margin:10px;height:220px">
                                            <div style="font-size:24px">@warehouse.WarehouseName</div>
                                            <div>@warehouse.WarehouseNo</div>
                                            <br />
                                            <div>@warehouse.Remark</div>
                                        </div>
                                        <div>
                                            <span class="btn btn-primary navbar-btn" style="background:skyblue">进入</span>
                                        </div>
                                    </div>
                                </a>
                            }
                        }

                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="box">
                    <div class="box-header with-border">
                        <h3 class="box-title">我的操作记录</h3>
                        <div class="box-tools pull-right">
                            <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip"
                                    title="Collapse">
                                <i class="fa fa-minus"></i>
                            </button>
                            <button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove">
                                <i class="fa fa-times"></i>
                            </button>
                        </div>
                    </div>
                    <div class="box-body">
                    </div>
                </div>
            </div>
        </div>
    </section>
</div>

